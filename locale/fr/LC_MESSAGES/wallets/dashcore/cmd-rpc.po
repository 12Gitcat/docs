msgid "start"
msgstr "start"

#: ../../wallets/dashcore/cmd-rpc.rst:558
msgid "Start mixing"
msgstr "Commencer le mélange"

#: ../../wallets/dashcore/cmd-rpc.rst:560
msgid "Stop mixing"
msgstr "Arrêter le mélange"

#: ../../wallets/dashcore/cmd-rpc.rst:561
msgid "reset"
msgstr "reset"

#: ../../wallets/dashcore/cmd-rpc.rst:562
