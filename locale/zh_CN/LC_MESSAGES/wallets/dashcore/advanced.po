msgid "keepassid=<name>"
msgstr ""

#: ../../wallets/dashcore/advanced.rst:420
msgid "KeePassHttp id for the established association"
msgstr "已建立关联的KeePassHttp身份"

#: ../../wallets/dashcore/advanced.rst:423
